@{
    ViewBag.Title = "User History";
}

<h2>@ViewBag.Title.</h2>
<table class="table table-striped table-bordered">
    <tr>
        <th>
            Activity
        </th>
        <th>
            Date
        </th>

    </tr>
    @foreach (var notification in Model)
    {
        <tr>
            <td>
                @notification.Activity
            </td>
            <td>
                @notification.ActivityTime.ToLocalTime()
            </td>
        </tr>
    }
</table>
