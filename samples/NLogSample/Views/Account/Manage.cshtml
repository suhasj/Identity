@{
    ViewBag.Title = "Manage Account";
}

<h2>@ViewBag.Title.</h2>
<p class="text-success">@ViewBag.StatusMessage</p>

<div class="row">
    <div class="col-md-12">
        @await Html.PartialAsync("_ChangePasswordPartial")
        <hr/>
@Html.ActionLink("View User History","UserHistory","Account",null,new {@class="btn btn-default"})
    </div>
</div>

@section Scripts {
    <script src="@Url.Content("~/lib/jquery.validation/jquery.validate.js")"></script>
    <script src="@Url.Content("~/js/jquery.validate.unobtrusive.js")"></script>
}
